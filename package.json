{
  "name": "entain-next-2-go",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run lint && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "lint": "eslint . --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "vite-bundle-analyzer",
    "clean": "rm -rf dist node_modules/.vite",
    "preinstall": "npx only-allow npm",
    "prepare": "npm run type-check && npm run lint:check && npm run format:check",
    "ci": "npm run type-check && npm run lint:check && npm run format:check && npm run test:run && npm run build",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:run"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.14",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "pinia": "^3.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
